{
  "name": "vcpkg-ci-freerdp",
  "version-string": "ci",
  "description": "Port to force features of certain ports within CI",
  "homepage": "https://github.com/microsoft/vcpkg",
  "dependencies": [
    {
      "name": "freerdp",
      "features": [
        "server"
      ]
    },
    {
      "name": "freerdp",
      "features": [
        "wayland",
        "x11"
      ],
      "platform": "linux"
    },
    {
      "name": "freerdp",
      "features": [
        "ffmpeg"
      ],
      "platform": "!windows"
    },
    {
      "name": "freerdp",
      "features": [
        "urbdrc"
      ],
      "platform": "!uwp & !xbox & !android"
    }
  ]
}
