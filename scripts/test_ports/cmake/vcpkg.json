{
  "name": "cmake",
  "version": "3.22.2",
  "port-version": 5,
  "description": "CMake is an open-source, cross-platform family of tools designed to build, test and package software.",
  "homepage": "https://cmake.org/",
  "license": "BSD-3-Clause",
  "dependencies": [
    {
      "name": "curl",
      "default-features": false
    },
    "expat",
    "jsoncpp",
    {
      "name": "libarchive",
      "default-features": false
    },
    "libuv",
    {
      "name": "ncurses",
      "platform": "!(windows | uwp)"
    },
    "rhash",
    {
      "name": "qtbase",
      "default-features": false
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ]
}
