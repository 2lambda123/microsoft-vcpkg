{
  "name": "vcpkg-ci-paraview",
  "version-string": "1",
  "port-version": 3,
  "description": "Port to force features of certain ports within CI",
  "homepage": "https://github.com/microsoft/vcpkg",
  "dependencies": [
    {
      "name": "mesa",
      "default-features": false,
      "features": [
        "egl",
        "gles1",
        "gles2",
        "llvm",
        "offscreen",
        "opengl"
      ],
      "platform": "windows"
    },
    {
      "name": "mesa",
      "default-features": false,
      "features": [
        "egl",
        "gles1",
        "gles2",
        "glx",
        "llvm",
        "offscreen",
        "opengl"
      ],
      "platform": "!windows"
    }
  ]
}
