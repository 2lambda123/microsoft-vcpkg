<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WindowsTargetPlatformVersion>@arg_TARGET_PLATFORM_VERSION@</WindowsTargetPlatformVersion>
    <PlatformToolset>@VCPKG_PLATFORM_TOOLSET@</PlatformToolset>
    <Platform>@arg_PLATFORM@</Platform>
    <VcpkgApplocalDeps>false</VcpkgApplocalDeps>
    <VcpkgManifestInstall>false</VcpkgManifestInstall>
    <VcpkgManifestEnabled>false</VcpkgManifestEnabled>
    <VcpkgEnabled>false</VcpkgEnabled>
    <VcpkgTriplet>@TARGET_TRIPLET@</VcpkgTriplet>
    <VcpkgInstalledDir>@_VCPKG_INSTALLED_DIR@</VcpkgInstalledDir>
    <UseIntelMKL>No</UseIntelMKL>
    <ImportProjectExtensionProps>false</ImportProjectExtensionProps>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
        <!-- vcpkg -->
        <RuntimeLibrary Condition="'${VCPKG_CRT_LINKAGE}|$(Configuration)' == 'static|${arg_DEBUG_CONFIGURATION}'">MultiThreadedDebug</RuntimeLibrary>
        <RuntimeLibrary Condition="'${VCPKG_CRT_LINKAGE}|$(Configuration)' == 'static|${arg_RELEASE_CONFIGURATION}'">MultiThreaded</RuntimeLibrary>
        <RuntimeLibrary Condition="'${VCPKG_CRT_LINKAGE}|$(Configuration)' == 'dynamic|${arg_DEBUG_CONFIGURATION}'">MultiThreadedDebugDLL</RuntimeLibrary>
        <RuntimeLibrary Condition="'${VCPKG_CRT_LINKAGE}|$(Configuration)' == 'dynamic|${arg_RELEASE_CONFIGURATION}'">MultiThreadedDLL</RuntimeLibrary>
        <AdditionalIncludeDirectories Condition="'$(Configuration)'=='${arg_DEBUG_CONFIGURATION}'">
            %(AdditionalIncludeDirectories);@_VCPKG_INSTALLED_DIR@/@TARGET_TRIPLET@/include;${MSBUILD_INCLUDE_DIRS_DEBUG}
        </AdditionalIncludeDirectories>
        <AdditionalIncludeDirectories Condition="'$(Configuration)'=='${arg_RELEASE_CONFIGURATION}'">
            %(AdditionalIncludeDirectories);@_VCPKG_INSTALLED_DIR@/@TARGET_TRIPLET@/include;${MSBUILD_INCLUDE_DIRS_RELEASE}
        </AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <ClCompile Update="**/*.cpp;**/*.cxx">
        <AdditionalOptions Condition="'$(Configuration)'=='${arg_DEBUG_CONFIGURATION}'">
            %(AdditionalOptions) @VCPKG_DETECTED_CMAKE_CXX_FLAGS_DEBUG@ @MSBUILD_CFLAGS_DEBUG@
        </AdditionalOptions>
        <AdditionalOptions Condition="'$(Configuration)'=='${arg_RELEASE_CONFIGURATION}'">
            %(AdditionalOptions) @VCPKG_DETECTED_CMAKE_CXX_FLAGS_RELEASE@ @MSBUILD_CFLAGS_RELEASE@
        </AdditionalOptions>
    </ClCompile>
  </ItemGroup>
  <ItemGroup >
    <ClCompile Update="**/*.c">
        <AdditionalOptions Condition="'$(Configuration)'=='${arg_DEBUG_CONFIGURATION}'">
            %(AdditionalOptions) @VCPKG_DETECTED_CMAKE_C_FLAGS_DEBUG@ @MSBUILD_CFLAGS_DEBUG@
        </AdditionalOptions>
        <AdditionalOptions Condition="'$(Configuration)'=='${arg_RELEASE_CONFIGURATION}'">
            %(AdditionalOptions) @VCPKG_DETECTED_CMAKE_C_FLAGS_RELEASE@ @MSBUILD_CFLAGS_RELEASE@
        </AdditionalOptions>
    </ClCompile>
  </ItemGroup>
  <ItemDefinitionGroup>
    <Link>
        <!-- Required Libs -->
        <AdditionalDependencies Condition="'$(Configuration)'=='${arg_DEBUG_CONFIGURATION}'">
            ${MSBUILD_LIBRARIES_DEBUG};@arg_ADDITIONAL_LIBS_DEBUG@;%(AdditionalDependencies)
        </AdditionalDependencies>
        <AdditionalDependencies Condition="'$(Configuration)'=='${arg_RELEASE_CONFIGURATION}'">
            ${MSBUILD_LIBRARIES_RELEASE};@arg_ADDITIONAL_LIBS_RELEASE@;%(AdditionalDependencies)
        </AdditionalDependencies >
        <AdditionalLibraryDirectories Condition="'$(Configuration)'=='${arg_RELEASE_CONFIGURATION}'">
            @MSBUILD_LIBRARIES_DIRS_RELEASE@;@_VCPKG_INSTALLED_DIR@/@TARGET_TRIPLET@/lib;$(AdditionalLibraryDirectories)
        </AdditionalLibraryDirectories>
        <AdditionalLibraryDirectories Condition="'$(Configuration)'=='${arg_DEBUG_CONFIGURATION}'">
            @MSBUILD_LIBRARIES_DIRS_DEBUG@;@_VCPKG_INSTALLED_DIR@/@TARGET_TRIPLET@/debug/lib;$(AdditionalLibraryDirectories)
        </AdditionalLibraryDirectories>
        <AdditionalOptions Condition="'$(Configuration)'=='${arg_DEBUG_CONFIGURATION}'">
            %(AdditionalOptions) @VCPKG_DETECTED_CMAKE_SHARED_LINKER_FLAGS_DEBUG@ @MSBUILD_LIBS_DEBUG@
        </AdditionalOptions>
        <AdditionalOptions Condition="'$(Configuration)'=='${arg_RELEASE_CONFIGURATION}'">
            %(AdditionalOptions) @VCPKG_DETECTED_CMAKE_SHARED_LINKER_FLAGS_RELEASE@ @MSBUILD_LIBS_RELEASE@
        </AdditionalOptions>
    </Link>
    <Lib>
      <!-- Required to prevent linker errors on Visual Studio 2017 with static CRT -->
      <TargetMachine Condition="'${VCPKG_TARGET_ARCHITECTURE}'=='x86'">MachineX86</TargetMachine>
      <TargetMachine Condition="'${VCPKG_TARGET_ARCHITECTURE}'=='x64'">MachineX64</TargetMachine>
      <AdditionalOptions Condition="'$(Configuration)'=='${arg_DEBUG_CONFIGURATION}'">
        %(AdditionalOptions) @VCPKG_DETECTED_CMAKE_STATIC_LINKER_FLAGS_DEBUG@
      </AdditionalOptions>
      <AdditionalOptions Condition="'$(Configuration)'=='${arg_RELEASE_CONFIGURATION}'">
        %(AdditionalOptions) @VCPKG_DETECTED_CMAKE_STATIC_LINKER_FLAGS_RELEASE@
      </AdditionalOptions>
    </Lib>
  </ItemDefinitionGroup>
  @VCPKG_MSBUILD_ADDITIONAL_TARGETS_XML@
</Project>