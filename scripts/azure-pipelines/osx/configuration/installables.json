{
  "$schema": "./installables.schema.json",

  "Applications": [
    {
      "Name": "vagrant",
      "VersionCommand": [ "vagrant", "-v" ],
      "VersionRegex": "Vagrant (.*)",
      "Version": "2.2.19",
      "DmgUrl": "https://releases.hashicorp.com/vagrant/2.2.19/vagrant_2.2.19_x86_64.dmg",
