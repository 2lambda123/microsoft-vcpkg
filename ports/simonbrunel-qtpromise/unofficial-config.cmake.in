
set(@PORT@_VERSION "@VERSION@")
set(@PORT@_DIR "${VCPKG_INSTALLED_DIR}/${VCPKG_TARGET_TRIPLET}/include/@PORT@" CACHE INTERNAL "")

if(TARGET unofficial-@PORT@)
    set(unofficial-@PORT@_FOUND TRUE)
    return()
endif()

include(CMakeFindDependencyMacro)

find_package(QT 5.6.0 NAMES Qt6 Qt5 REQUIRED)
find_package(Qt${QT_VERSION_MAJOR} REQUIRED COMPONENTS Core)

set(CMAKE_AUTOMOC ON)
set(CMAKE_CXX_STANDARD 11)
set(CMAKE_CXX_STANDARD_REQUIRED ON)

add_library(unofficial-@PORT@ INTERFACE)

target_link_libraries(unofficial-@PORT@ INTERFACE Qt${QT_VERSION_MAJOR}::Core)
target_include_directories(unofficial-@PORT@ INTERFACE "${@PORT@_DIR}/include")


set(unofficial-@PORT@_FOUND TRUE)

