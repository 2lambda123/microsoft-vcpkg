set(REQUIRES )
foreach(ARG ${ARGS})
    if (ARG STREQUAL "REQUIRED")
        set(REQUIRES "REQUIRED")
        break()
    endif()
endforeach()

_find_package(unofficial-lua CONFIG ${REQUIRES})

set(LUA_LIBRARIES lua)
set(LUA_FOUND 1)

if (@ENABLE_LUA_CPP@)
    _find_package(unofficial-lua-cpp CONFIG ${REQUIRES})
    list(APPEND LUA_LIBRARIES lua-cpp)
endif()