{
  "name": "boost-fiber",
  "version": "1.78.0",
  "port-version": 1,
  "description": "Boost fiber module",
  "homepage": "https://github.com/boostorg/fiber",
  "supports": "!osx & !uwp & !arm & !emscripten",
  "dependencies": [
    "boost-algorithm",
    "boost-assert",
    {
      "name": "boost-build",
      "host": true
    },
    "boost-config",
    {
      "name": "boost-context",
      "platform": "!uwp & !emscripten"
    },
    "boost-core",
    {
      "name": "boost-filesystem",
      "platform": "!uwp"
    },
    "boost-format",
    "boost-intrusive",
    "boost-predef",
    "boost-smart-ptr",
    {
      "name": "vcpkg-boost-build",
      "host": true
    },
    {
      "name": "vcpkg-boost-copy",
      "host": true
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ],
  "features": {
    "numa": {
      "description": "Enable NUMA support"
    }
  }
}
