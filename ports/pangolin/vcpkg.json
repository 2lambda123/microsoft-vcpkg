{
  "name": "pangolin",
  "version": "0.6",
  "description": "Lightweight GUI Library",
  "homepage": "https://github.com/stevenlovegrove/Pangolin",
  "supports": "!uwp & !osx",
  "dependencies": [
    "eigen3",
    {
      "name": "ffmpeg",
      "features": [
        "avformat"
      ]
    },
    {
        "name": "vcpkg-cmake",
        "host": true
    },
    {
       "name": "vcpkg-cmake-config",
       "host": true
    },
    "glew",
    "libjpeg-turbo",
    "libpng"
  ]
}
