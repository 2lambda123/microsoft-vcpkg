# - Config file for the stormlib package
# It defines the following variables
#  STORMLIB_INCLUDE_DIRS - include directories for STORMLIB
#  STORMLIB_LIBRARIES    - libraries to link against

# Load targets
get_filename_component(STORMLIB_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include("${STORMLIB_CMAKE_DIR}/stormlibTargets.cmake")

# Set properties
get_target_property(STORMLIB_INCLUDE_DIRS stormlib INTERFACE_INCLUDE_DIRECTORIES)
set(STORMLIB_LIBRARIES stormlib)
mark_as_advanced(STORMLIB_INCLUDE_DIRS STORMLIB_LIBRARIES)
