{
  "name": "chakracore",
  "version-date": "2022-11-09",
  "port-version": 2,
  "description": "ChakraCore is an open source Javascript engine with a C API.",
  "homepage": "https://github.com/chakra-core/ChakraCore",
  "license": "MIT",
  "supports": "!uwp",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "icu",
    "jit"
  ],
  "features": {
    "icu": {
      "description": "Enable Unicode and Intl. (Ignored for Windows.)",
      "dependencies": [
        "icu"
      ]
    },
    "jit": {
      "description": "Enable JIT. (Ignored for Windows.)"
    },
    "tools": {
      "description": "Build tools. (Ignored for Windows.)"
    }
  }
}
