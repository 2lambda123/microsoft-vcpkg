{
  "name": "itay-grudev-singleapplication",
  "version": "3.5.1",
  "description": "Replacement of QtSingleApplication for Qt 5 and Qt 6 with support for inter-instance communication",
  "homepage": "https://github.com/itay-grudev/SingleApplication",
  "license": "MIT",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "qcoreapplication"
  ],
  "features": {
    "qapplication": {
      "description": "SingleApplication class inherits from QApplication.",
      "dependencies": [
        {
          "name": "qtbase",
          "default-features": false,
          "features": [
            "network",
            "widgets"
          ]
        }
      ]
    },
    "qcoreapplication": {
      "description": "SingleApplication class inherits from QCoreApplication.",
      "dependencies": [
        {
          "name": "qtbase",
          "default-features": false,
          "features": [
            "network"
          ]
        }
      ]
    },
    "qguiapplication": {
      "description": "SingleApplication class inherits from QGuiApplication.",
      "dependencies": [
        {
          "name": "qtbase",
          "default-features": false,
          "features": [
            "gui",
            "network"
          ]
        }
      ]
    }
  }
}
