{
  "name": "gdal",
  "version": "3.2.1",
  "description": "The Geographic Data Abstraction Library for reading and writing geospatial raster and vector data",
  "homepage": "https://gdal.org",
  "supports": "!arm",
  "dependencies": [
    "proj4",
    "libpng",
    "geos",
    "sqlite3",
    "curl",
    "expat",
    "libpq",
    "openjpeg",
    "libwebp",
    "libxml2",
    "liblzma",
    "netcdf-c",
    "hdf5",
    "zlib",
    "libgeotiff",
    "cfitsio",
    "zstd",
    {
      "name": "json-c",
      "platform": "!windows"
    }
  ],
  "features": {
    "mysql-libmariadb": {
      "description": "Add mysql support using libmariadb",
      "dependencies": [
        "libmariadb"
      ]
    },
    "libspatialite": {
      "description": "Create or update SpatiaLite databases using libspatialite",
      "dependencies": [
        "libspatialite"
      ]
    }
  }
}
