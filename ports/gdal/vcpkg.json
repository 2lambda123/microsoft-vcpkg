{
  "name": "gdal",
  "version-semver": "3.2.2",
  "port-version": 1,
  "description": "The Geographic Data Abstraction Library for reading and writing geospatial raster and vector data",
  "homepage": "https://gdal.org",
  "supports": "!arm",
  "dependencies": [
    "cfitsio",
    "curl",
    "expat",
    "geos",
    "giflib",
    {
      "name": "json-c",
      "platform": "!windows"
    },
    "libgeotiff",
    "liblzma",
    "libpng",
    "libpq",
    "libwebp",
    "libxml2",
    "openjpeg",
    "proj4",
    "sqlite3",
    "zlib",
    "zstd"
  ],
  "features": {
    "hdf5": {
      "description": "Add HDF5 and nc files support",
      "dependencies": [
        "hdf5",
        "netcdf-c"
      ]
    },
    "libspatialite": {
      "description": "Create or update SpatiaLite databases using libspatialite",
      "dependencies": [
        "libspatialite"
      ]
    },
    "mysql-libmariadb": {
      "description": "Add mysql support using libmariadb",
      "dependencies": [
        "libmariadb"
      ]
    }
  }
}
