{
  "name": "ms-gltf",
  "version": "28-06-2022",
  "description": "glTF-SDK is a C++ Software Development Kit for glTF",
  "homepage": "https://github.com/microsoft/glTF-SDK",
  "supports": "!linux",
  "dependencies": [
    "rapidjson",
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ],
  "default-features": [
    "test"
  ],
  "features": {
    "samples": {
      "description": "Build with GLTF samples"
    },
    "test": {
      "description": "Build test programs",
      "dependencies": [
        "gtest"
      ]
    }
  }
}
