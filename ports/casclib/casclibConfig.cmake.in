# - Config file for the casclib package
# It defines the following variables
#  CASCLIB_INCLUDE_DIRS - include directories for CASCLIB
#  CASCLIB_LIBRARIES    - libraries to link against

# Load targets
get_filename_component(CASCLIB_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include("${CASCLIB_CMAKE_DIR}/casclibTargets.cmake")

# Set properties
get_target_property(CASCLIB_INCLUDE_DIRS casclib INTERFACE_INCLUDE_DIRECTORIES)
set(CASCLIB_LIBRARIES casclib)
mark_as_advanced(CASCLIB_INCLUDE_DIRS CASCLIB_LIBRARIES)
