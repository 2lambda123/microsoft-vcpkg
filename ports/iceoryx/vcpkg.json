{
  "name": "iceoryx",
  "version": "2.0.2",
  "description": "True zero-copy inter-process-communication",
  "homepage": "https://iceoryx.io",
  "license": "Apache-2.0",
  "supports": "!uwp",
  "dependencies": [
    {
      "name": "acl",
      "platform": "linux"
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "toml-config"
  ],
  "features": {
    "one-to-many-only": {
      "description": "Restricts communication to 1:n pattern"
    },
    "toml-config": {
      "description": "TOML support for RouDi with dynamic configuration",
      "dependencies": [
        "cpptoml"
      ]
    }
  }
}
