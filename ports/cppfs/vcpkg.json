{
  "name": "cppfs",
  "version-string": "1.3.0",
  "port-version": 1,
  "description": "Cross-platform C++ file system library supporting multiple backends",
  "supports": "!uwp",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "ssh": {
      "description": "SSH backend for cppfs",
      "dependencies": [
        "libssh2",
        "openssl",
        "zlib"
      ]
    }
  }
}
