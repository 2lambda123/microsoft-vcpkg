{
  "name": "poppler",
  "version-string": "20.12.1",
  "description": "a PDF rendering library",
  "homepage": "https://poppler.freedesktop.org/",
  "dependencies": [
    {
      "name": "cairo",
      "platform": "osx"
    },
    {
      "name": "devil",
      "platform": "(windows | linux) & !arm"
    },
    {
      "name": "fontconfig",
      "platform": "!(windows | android)"
    },
    "freetype",
    "libiconv",
    "openjpeg"
  ],
  "features": {
    "zlib": {
      "description": "zlib for poppler",
      "dependencies": [
        "zlib"
      ]
    }
  }
}
