{
  "name": "poco",
  "version": "1.12.4",
  "port-version": 4,
  "description": "Modern, powerful open source C++ class libraries for building network and internet-based applications that run on desktop, server, mobile and embedded systems.",
  "homepage": "https://github.com/pocoproject/poco",
  "license": "BSL-1.0",
  "supports": "!uwp",
  "dependencies": [
    "expat",
    "openssl",
    "pcre2",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    "zlib"
  ],
  "features": {
    "mariadb": {
      "description": "MariaDB support for POCO",
      "dependencies": [
        "libmariadb"
      ]
    },
    "mysql": {
      "description": "Mysql support for POCO",
      "dependencies": [
        "libmysql"
      ]
    },
    "netsslwin": {
      "description": "NetSSL Windows support for POCO (replaces NetSSL)",
      "supports": "windows"
    },
    "pdf": {
      "description": "Haru support for POCO",
      "dependencies": [
        "libharu"
      ]
    },
    "postgresql": {
      "description": "PostgreSQL support for POCO",
      "dependencies": [
        "libpqxx"
      ]
    },
    "sqlite3": {
      "description": "Sqlite3 support for POCO",
      "dependencies": [
        "sqlite3"
      ]
    }
  }
}
