{
  "name": "netcdf-c",
  "version": "4.8.1",
  "description": "A set of self-describing, machine-independent data formats that support the creation, access, and sharing of array-oriented scientific data.",
  "homepage": "https://github.com/Unidata/netcdf-c",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "dap",
    "hdf5",
    "nczarr",
    "netcdf-4"
  ],
  "features": {
    "dap": {
      "description": "Build with DAP remote access client support",
      "dependencies": [
        "curl"
      ]
    },
    "hdf5": {
      "description": "Build with HDF5 support",
      "dependencies": [
        {
          "name": "hdf5",
          "features": [
            "zlib"
          ]
        },
        {
          "name": "netcdf-c",
          "default-features": false,
          "features": [
            "netcdf-4"
          ]
        }
      ]
    },
    "nczarr": {
      "description": "Build with NCZarr cloud storage access support",
      "dependencies": [
        {
          "name": "netcdf-c",
          "default-features": false,
          "features": [
            "netcdf-4"
          ]
        }
      ]
    },
    "nczarr-s3": {
      "description": "Build with NCZarr S3 support",
      "dependencies": [
        "aws-sdk-cpp",
        {
          "name": "netcdf-c",
          "default-features": false,
          "features": [
            "nczarr"
          ]
        }
      ]
    },
    "nczarr-zip": {
      "description": "Build with NCZarr ZIP support",
      "dependencies": [
        "libzip",
        {
          "name": "netcdf-c",
          "default-features": false,
          "features": [
            "nczarr"
          ]
        }
      ]
    },
    "netcdf-4": {
      "description": "Build with netCDF-4 support"
    },
    "tools": {
      "description": "Build utilities"
    }
  }
}
