{
  "name": "restc-cpp",
  "version-string": "0.10.0",
  "description": "Modern C++ REST Client library",
  "homepage": "https://github.com/jgaa/restc-cpp",
  "license": "MIT",
  "supports": "static",
  "dependencies": [
    "boost-chrono",
    "boost-context",
    "boost-coroutine",
    "boost-date-time",
    "boost-filesystem",
    "boost-program-options",
    "boost-system",
    "boost-uuid",
    "rapidjson"
  ],
  "default-features": [
    "boost-log",
    "openssl",
    "zlib"
  ],
  "features": {
    "boost-log": {
      "description": "Use boost::log for logging.",
      "dependencies": [
        "boost-log"
      ]
    },
    "clog": {
      "description": "Use std::clog for logging."
    },
    "cxx17": {
      "description": "Use the C++17 standard (default is C++14)."
    },
    "openssl": {
      "description": "OpenSSL support.",
      "dependencies": [
        "openssl"
      ]
    },
    "threaded-ctx": {
      "description": "Allow asio contexts with multiple threads. Enables thread-safe internal access."
    },
    "zlib": {
      "description": "Use zlib.",
      "dependencies": [
        "zlib"
      ]
    }
  }
}
