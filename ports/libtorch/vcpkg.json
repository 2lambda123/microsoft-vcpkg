{
  "name": "libtorch",
  "version-string": "v1.8.1",
  "description": "Tensors and Dynamic neural networks in Python with strong GPU acceleration",
  "homepage": "https://pytorch.org/",
  "supports": "x64 & (windows | osx | linux)",
  "dependencies": [
    "cpuinfo",
    "eigen3",
    "fbgemm",
    "fmt",
    "fp16",
    "gflags",
    "glog",
    "lmdb",
    {
      "name": "onnx",
      "features": [
        "foxi"
      ]
    },
    "onnx-optimizer",
    {
      "name": "protobuf",
      "host": true
    },
    "protobuf",
    "sleef",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    "zstd"
  ],
  "default-features": [
    "xnnpack"
  ],
  "features": {
    "cuda": {
      "description": "Build with NVIDIA CUDA GPU backend",
      "dependencies": [
        "cuda",
        "cudnn",
        {
          "name": "tensorpipe",
          "features": [
            "cuda"
          ]
        }
      ]
    },
    "dist": {
      "description": "Use distributed with MPI, Gloo, libuv, TensorPipe",
      "dependencies": [
        {
          "name": "gloo",
          "platform": "linux"
        },
        {
          "name": "libuv",
          "platform": "windows | osx"
        },
        {
          "name": "tensorpipe",
          "platform": "linux | osx"
        }
      ]
    },
    "leveldb": {
      "description": "Build with LevelDB",
      "dependencies": [
        "leveldb",
        "snappy"
      ]
    },
    "nnpack": {
      "description": "Build with NNPack",
      "dependencies": [
        "nnpack"
      ]
    },
    "opencl": {
      "description": "Build with OpenCL",
      "dependencies": [
        "opencl"
      ]
    },
    "opencv3": {
      "description": "Build with OpenCV 3.x",
      "dependencies": [
        "opencv3"
      ]
    },
    "python": {
      "description": "Build with Python3 + NumPy"
    },
    "qnnpack": {
      "description": "Build with QNNPack",
      "dependencies": [
        "qnnpack"
      ]
    },
    "tbb": {
      "description": "Build with Intel TBB",
      "dependencies": [
        "tbb"
      ]
    },
    "vulkan": {
      "description": "Build with Vulkan GPU backend",
      "dependencies": [
        "vulkan"
      ]
    },
    "xnnpack": {
      "description": "Build with XNNPack",
      "dependencies": [
        "xnnpack"
      ]
    },
    "zstd": {
      "description": "Build with ZSTD",
      "dependencies": [
        "zstd"
      ]
    }
  }
}
