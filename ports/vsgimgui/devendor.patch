-//    };
-//    ...
-//    if (ImPlot::BeginPlot("MyPlot")) {
-//        MyData my_data;
-//        ImPlot::PlotScatterG("scatter", MyDataGetter, &my_data, my_data.Size());
-//        ImPlot::PlotLineG("line", my_lambda, nullptr, 1000);
-//        ImPlot::EndPlot();
-//    }
