{
  "name": "mygui",
  "version": "3.4.1",
  "port-version": 5,
  "description": "Fast, flexible and simple GUI",
  "homepage": "http://mygui.info",
  "license": "MIT",
  "supports": "!uwp",
  "dependencies": [
      "freetype",
      "sdl2-image",
      {
          "name": "vcpkg-cmake",
          "host": true
      },
      {
          "name": "vcpkg-cmake-config",
          "host": true
      }
  ],
  "features": {
      "opengl": {
          "description": "Use OpenGL render system.",
          "dependencies": [
              "opengl"
          ]
      },
      "opengles": {
          "description": "Use OpenGL ES render system (recommended for Emscripten).",
          "dependencies": [
              {
                  "name": "opengl",
                  "platform": "!emscripten"
              }
          ]
      },
      "tools": {
          "description": "Install MyGUI tools."
      }
  }
}
