{
  "name": "libcoap",
  "version": "4.3.0",
  "description": "A CoAP (RFC 7252) implementation in C",
  "homepage": "https://github.com/obgm/libcoap",
  "documentation": "https://libcoap.net",
  "license": "BSD-2-Clause",
  "supports": "!uwp",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "dtls",
    "epoll",
    "tcp"
  ],
  "features": {
    "dtls": {
      "description": "compile with dtls support",
      "dependencies": [
        "openssl"
      ]
    },
    "epoll": {
      "description": "compile with epoll support"
    },
    "examples": {
      "description": "Build examples (e.g. coap-client, coap-server)"
    },
    "tcp": {
      "description": "Enable building with TCP support"
    }
  }
}
