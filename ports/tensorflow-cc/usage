    set(CMAKE_PREFIX_PATH ${CMAKE_PREFIX_PATH}
        # e.g. vcpkg/installed
        <installed_package_path>
    )

    find_package(
        TensorflowCC REQUIRED
        CONFIG
        PATH_SUFFIXES share/tensorflow-cc
    )

    target_link_libraries(
        <TARGET_NAME>
        PRIVATE
            tensorflow_cc::tensorflow_cc
    )
