{
    "name": "avcpp",
    "version-date": "2021-05-06",
    "homepage": "https://github.com/h4tr3d/avcpp",
    "description": "Wrapper for the FFmpeg that simplify usage it from C++ projects.",
    "dependencies": [
        {
            "name": "ffmpeg",
            "default-features": false,
            "features": [
                "avcodec",
                "avformat",
                "avdevice",
                "avfilter",
                "swscale",
                "swresample"
            ]
        },
        {
            "name": "vcpkg-cmake",
            "host": true
          },
          {
            "name": "vcpkg-cmake-config",
            "host": true
          }
    ]
}