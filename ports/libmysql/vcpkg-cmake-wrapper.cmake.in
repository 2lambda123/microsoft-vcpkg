if (@BUILD_STATIC_LIBS@)
    set(LIBMYSQL_PREV_PREFIX_PATH ${CMAKE_PREFIX_PATH})
    list(APPEND CMAKE_PREFIX_PATH ${CMAKE_CURRENT_LIST_DIR}/../unofficial-mysqlclient)
    find_package(unofficial-mysqlclient CONFIG REQUIRED)
    set(CMAKE_PREFIX_PATH ${LIBMYSQL_PREV_PREFIX_PATH})
    set(MYSQL_LIBRARY mysqlclient)
else()
    find_package(unofficial-libmysql CONFIG REQUIRED)
    set(MYSQL_LIBRARY libmysql)
endif()

set(MYSQL_LIBRARIES ${MYSQL_LIBRARY})