{
  "name": "gstreamer",
  "version": "1.18.4",
  "description": "GStreamer open-source multimedia framework core library",
  "homepage": "https://gstreamer.freedesktop.org/",
  "supports": "(windows | osx) & (x64 | x86)",
  "dependencies": [
    "brotli",
    {
      "name": "cairo",
      "features": [
        "freetype"
      ],
      "platform": "osx"
    },
    {
      "name": "cairo",
      "features": [
        "gobject"
      ],
      "platform": "windows"
    },
    {
      "name": "graphene",
      "platform": "windows"
    },
    "jack2",
    "libflac",
    "libjpeg-turbo",
    "libpng",
    "libraw",
    "libvpx",
    "libxml2",
    "mpg123",
    {
      "name": "opengl-registry",
      "platform": "windows & osx"
    },
    "speex",
    "taglib",
    "tool-meson",
    {
      "name": "yasm-tool-helper",
      "platform": "windows"
    }
  ]
}
