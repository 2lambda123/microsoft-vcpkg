{
  "name": "gstreamer",
  "version": "1.18.4",
  "description": "GStreamer open-source multimedia framework core library",
  "homepage": "https://gstreamer.freedesktop.org/",
  "supports": "!(arm | uwp | linux)",
  "dependencies": [
    "brotli",
    {
      "name": "cairo",
      "features": [
        "freetype"
      ]
    },
    "jack2",
    "libflac",
    "libjpeg-turbo",
    "libpng",
    "libraw",
    "libvpx",
    "libxml2",
    "mpg123",
    {
      "name": "opengl-registry",
      "platform": "windows"
    },
    "speex",
    "taglib",
    "tool-meson"
  ]
}
