{
  "name": "vtk-dicom",
  "version-string": "0.8.13",
  "description": "DICOM for VTK",
  "homepage": "https://github.com/dgobbi/vtk-dicom",
  "dependencies": [
    {
      "name": "vtk",
      "default-features": false
    },
    {
      "name": "python3",
      "default-features": false,
      "host" : true
    },
    "zlib"
  ],
  "features": {
    "gdcm": {
      "description": "Use gdcm for decompressing DICOM files.",
      "dependencies": [
        "gdcm"
      ]
    }
  }
}
