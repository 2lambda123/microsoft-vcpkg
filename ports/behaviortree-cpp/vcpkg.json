{
  "name": "behaviortree-cpp",
  "version": "3.6.0",
  "description": "Behavior Trees Library in C++.",
  "homepage": "https://www.behaviortree.dev",
  "license": "MIT",
  "supports": "!uwp",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "coroutines": {
      "description": "enables coroutine support",
      "dependencies": [
        "boost-coroutine2"
      ]
    },
    "curses": {
      "description": "Adds curses (ManualNode)",
      "dependencies": [
        "ncurses"
      ]
    },
    "examples": {
      "description": "build all examples"
    },
    "recorder": {
      "description": "build PublisherZMQ and bt_recorder.",
      "dependencies": [
        "cppzmq"
      ]
    },
    "tools": {
      "description": "builds the tools"
    }
  }
}
